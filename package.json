{
  "name": "raiolib",
  "version": "1.1.5-b",
  "description": "Raio lib é um pacote npm que contém uma coleção de códigos modulares e genéricos para serem reutilizados em projetos TypeScript e JavaScript, com foco especial em NestJS e NextJS.",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "test": "vitest --watch",
    "test:cov": "vitest --watch --coverage",
    "prepublishOnly": "npm run build",
    "dev": "tsx --watch src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/raioramalho/raiolib.git"
  },
  "keywords": [
    "raioramalho",
    "sdk",
    "raio",
    "typescript"
  ],
  "author": "Alan Ramalho",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/raioramalho/raiolib/issues"
  },
  "homepage": "https://github.com/raioramalho/raiolib#readme",
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@vitest/coverage-v8": "^2.0.4",
    "bun": "^1.1.20",
    "ts-node": "^10.9.1",
    "tsx": "^4.16.2",
    "typescript": "^5.5.4",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^2.0.4"
  }
}
